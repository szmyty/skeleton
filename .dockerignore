# Ignore all node_modules except for the workspace
**/node_modules
!workspace/node_modules

# Ignore dependency files that should be installed fresh
**/.pnp
**/.pnp.js
**/.pnp.cjs
.pnpm-store/

# Ignore build artifacts
dist/
out/
build/
coverage/
.vscode/
.idea/
*.swp
*.swo

# Ignore logs
npm-debug.log
yarn-debug.log
yarn-error.log
pnpm-debug.log
lerna-debug.log

# Ignore environment files (secrets should never be in Docker images)
.env
.env.local
.env.development
.env.production
.env.test
.env*.local

# Ignore editor/system files
.DS_Store
Thumbs.db
*.log
*.pid
*.seed
*.pid.lock
*.swp
*.swo
*.swn
*.bak

# Ignore Pulumi state (if not using remote backend)
.pulumi/

# Ignore Kubernetes config (if using local kubeconfig)
.kube/

# Ignore Python-related files (if relevant)
**/__pycache__/
**/*.pyc
**/*.pyo
**/*.pyd

# Ignore Git-related files
.git
.gitignore
.gitmodules
.gitattributes
